Cell["intMonomio \[EGrave] la vera routine di integrazione", "Text",
 CellChangeTimes->{{3.882707368724497*^9, 
  3.882707379609297*^9}},ExpressionUUID->"cca54b5d-6076-48e9-8698-\
2f8417e09cd5"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"intMonomio", "[", 
    RowBox[{"expr_", ",", "var_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"prefattore", ",", "esponente", ",", "risultato"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"MatchQ", "[", 
         RowBox[{"expr", ",", 
          RowBox[{"p_.", " ", 
           RowBox[{"var", "^", "n_."}]}]}], "]"}], ",", 
        RowBox[{"(*", " ", 
         RowBox[{
          RowBox[{
          "Identifichiamo", " ", "il", " ", "prefattore", " ", "e", " ", 
           RowBox[{"l", "'"}], "esponente"}], ",", " ", 
          RowBox[{
          "per", " ", "farlo", " ", "devo", " ", "verificare", " ", "che", 
           " ", "il", " ", "pattern", " ", "viene", " ", "trovato"}]}], " ", 
         "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"prefattore", "=", 
          RowBox[{"expr", "/.", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"p_.", " ", 
              RowBox[{"var", "^", "n_."}]}], "\[Rule]", "p"}], "}"}]}]}], ";",
          "\[IndentingNewLine]", 
         RowBox[{"esponente", "=", 
          RowBox[{"expr", "/.", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"p_.", " ", 
              RowBox[{"var", "^", "n_."}]}], "\[Rule]", "n"}], "}"}]}]}], 
         ";"}]}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Which", "[", 
       RowBox[{"(*", " ", 
        RowBox[{
        "distinguiamo", " ", "i", " ", "vari", " ", "casi", " ", "che", " ", 
         "riusciamo", " ", "a", " ", "gestire"}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"MatchQ", "[", 
          RowBox[{"expr", ",", 
           RowBox[{"p_.", 
            RowBox[{"var", "^", "n_."}]}]}], "]"}], "&&", 
         RowBox[{"FreeQ", "[", 
          RowBox[{"prefattore", ",", "var"}], "]"}], "&&", 
         RowBox[{"FreeQ", "[", 
          RowBox[{"esponente", ",", "var"}], "]"}], "&&", 
         RowBox[{"esponente", "=!=", 
          RowBox[{"-", "1"}]}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"risultato", "=", 
         RowBox[{"prefattore", " ", 
          RowBox[{"1", "/", 
           RowBox[{"(", 
            RowBox[{"esponente", "+", "1"}], ")"}]}], " ", 
          RowBox[{"var", "^", 
           RowBox[{"(", 
            RowBox[{"esponente", "+", "1"}], ")"}]}]}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"MatchQ", "[", 
          RowBox[{"expr", ",", 
           RowBox[{"p_.", 
            RowBox[{"var", "^", "n_."}]}]}], "]"}], "&&", 
         RowBox[{"FreeQ", "[", 
          RowBox[{"prefattore", ",", "var"}], "]"}], "&&", 
         RowBox[{"esponente", "===", 
          RowBox[{"-", "1"}]}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"risultato", "=", " ", 
         RowBox[{"prefattore", " ", 
          RowBox[{"Log", "[", "var", "]"}]}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"FreeQ", "[", 
         RowBox[{"expr", ",", "var"}], "]"}], ",", "\[IndentingNewLine]", 
        RowBox[{"risultato", "=", 
         RowBox[{"expr", " ", "var"}]}], ",", "\[IndentingNewLine]", "True", 
        ",", 
        RowBox[{"(*", 
         RowBox[{
         "tutti", " ", "i", " ", "casi", " ", "che", " ", "non", " ", 
          "rientrano", " ", "nei", " ", "tre", " ", "casi", " ", 
          "specifici"}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{"risultato", "=", 
         RowBox[{"integra", "[", 
          RowBox[{"expr", ",", "var"}], "]"}]}]}], 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{"lascio", " ", 
          RowBox[{"l", "'"}], "espressione", " ", "in", " ", "maniera", " ", 
          "puramente", " ", "simbolica"}], ",", " ", 
         RowBox[{"come", " ", "lasciare", " ", "f", 
          RowBox[{"(", "x", ")"}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "risultato", "]"}]}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.882705955752913*^9, 3.882706012835288*^9}, {
   3.882707116659567*^9, 3.882707128383572*^9}, {3.882707358043474*^9, 
