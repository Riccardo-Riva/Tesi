Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"myInt", "[", 
    RowBox[{"expr_", ",", "var_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"temp", ",", "ris"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"temp", "=", 
       RowBox[{"Expand", "[", "expr", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Which", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"Head", "[", "temp", "]"}], "===", "Times"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"ris", "=", 
         RowBox[{"intMonomio", "[", 
          RowBox[{"temp", ",", "var"}], "]"}]}], ",", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Head", "[", "temp", "]"}], "===", "Plus"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"ris", "=", 
         RowBox[{"Map", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"intMonomio", "[", 
             RowBox[{"#", ",", "var"}], "]"}], "&"}], ",", "temp"}], "]"}]}], 
        ",", "\[IndentingNewLine]", "\[IndentingNewLine]", "True", ",", 
        "\[IndentingNewLine]", 
        RowBox[{"ris", "=", 
         RowBox[{"intMonomio", "[", 
          RowBox[{"temp", ",", "var"}], "]"}]}]}], "\[IndentingNewLine]", 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "ris", "]"}], ";"}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}]], "Input",
